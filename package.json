{
    "private": true,
    "name": "@xcpcio/xcpcio",
    "description": "XCPCIO",
    "version": "0.3.3",
    "packageManager": "pnpm@8.6.7",
    "license": "MIT",
    "author": "Dup4 <lyuzhi.pan@gmail.com>",
    "keywords": [
        "ICPC",
        "CCPC"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xcpcio/xcpcio.git"
    },
    "homepage": "https://github.com/xcpcio/xcpcio",
    "bugs": {
        "url": "https://github.com/xcpcio/xcpcio/issues"
    },
    "engines": {
        "node": "^16.16.0"
    },
    "scripts": {
        "build": "pnpm -r --filter=./packages/**/* run build",
        "build:libs": "pnpm -r --filter=./packages/libs/**/* run build",
        "dev": "pnpm -r --parallel --filter=./packages/**/* run dev",
        "start": "pnpm -r --parallel --filter=./packages/**/* run start",
        "test": "vitest",
        "lint": "run-p lint:*",
        "lint:build": "pnpm -r --parallel --filter=./packages/**/* run lint:build",
        "lint:eslint": "eslint .",
        "lint:style": "prettier --check ./packages/**/src/**/*.ts",
        "format": "run-p format:*",
        "format:eslint": "eslint --fix .",
        "format:style": "prettier --write ./packages/**/*.ts",
        "prepublishOnly": "npm run build",
        "release": "bumpp package.json packages/**/*/package.json --commit --push --tag && pnpm -r publish --access public"
    },
    "devDependencies": {
        "@babel/types": "^7.22.5",
        "@types/glob": "^7.2.0",
        "@types/node": "^17.0.45",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "bumpp": "^7.2.0",
        "eslint": "^8.44.0",
        "eslint-config-next": "13.4.9",
        "esmo": "^0.14.1",
        "fsxx": "^0.0.5",
        "glob": "^8.1.0",
        "npm-run-all": "^4.1.5",
        "pnpm": "^8.6.7",
        "prettier": "^2.8.8",
        "taze": "^0.10.3",
        "typescript": "^4.9.5",
        "unbuild": "^0.7.6",
        "vite": "^4.4.2",
        "vitepress": "1.0.0-beta.6",
        "vitest": "^0.32.4",
        "zx": "^6.2.5"
    }
}
