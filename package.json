{
    "name": "@xcpcio/board",
    "version": "0.1.0",
    "packageManager": "pnpm@7.1.2",
    "description": "XCPCIO Board",
    "repository": "https://github.com/XCPCIO/board",
    "author": "Dup4 <lyuzhi.pan@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": ">=16.11.0"
    },
    "files": [
        "src",
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "postinstall": "umi generate tmp",
        "dev": "cross-env UMI_UI=none umi dev",
        "start": "cross-env MOCK=none UMI_UI=none umi dev",
        "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 UMI_ENV=build umi build",
        "lint": "run-p lint:*",
        "lint:style": "prettier --check src/",
        "format": "run-p format:*",
        "format:style": "prettier --write src/",
        "test": "umi-test",
        "test:coverage": "umi-test --coverage"
    },
    "dependencies": {
        "@ahooksjs/use-url-state": "^2.5.7",
        "@ant-design/icons": "^4.7.0",
        "@ant-design/pro-layout": "^6.5.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.13",
        "antd": "^4.17.2",
        "browserslist": "^4.18.1",
        "caniuse-lite": "^1.0.30001283",
        "dayjs": "^1.10.7",
        "dva-core": "^1.1.0",
        "highcharts": "^8.2.0",
        "highcharts-react-official": "^3.0.0",
        "lodash": "^4.17.20",
        "react": "17.x",
        "react-dom": "17.x",
        "react-highlight-words": "^0.16.0",
        "react-router": "5.0.0",
        "redbox-react": "1.x",
        "umi": "^3.5.20",
        "umi-request": "^1.4.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.177",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-highlight-words": "^0.16.3",
        "@types/uuid": "^8.3.0",
        "@umijs/preset-react": "1.x",
        "@umijs/test": "^3.5.20",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.0",
        "typescript": "^4.1.2"
    }
}
