{
  "name": "xcpcio-board",
  "version": "v0.0.30.4",
  "description": "XCPCIO Board",
  "repository": "https://github.com/XCPCIO/XCPCIO-Board",
  "author": "Dup4",
  "license": "MIT",
  "scripts": {
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "dev": "cross-env UMI_UI=none umi dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^2.5.7",
    "@ant-design/pro-layout": "^6.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "antd": "^4.17.2",
    "browserslist": "^4.18.1",
    "caniuse-lite": "^1.0.30001283",
    "dayjs": "^1.10.7",
    "highcharts": "^8.2.0",
    "highcharts-react-official": "^3.0.0",
    "lodash": "^4.17.20",
    "react": "17.x",
    "react-dom": "17.x",
    "react-highlight-words": "^0.16.0",
    "umi": "^3.5.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-highlight-words": "^0.16.3",
    "@types/uuid": "^8.3.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=16.11.0"
  }
}
