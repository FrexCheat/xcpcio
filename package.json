{
    "name": "@xcpcio/board",
    "version": "0.1.11",
    "packageManager": "pnpm@7.32.4",
    "description": "XCPCIO Board",
    "repository": "https://github.com/XCPCIO/board",
    "author": "Dup4 <lyuzhi.pan@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": "^16.16.0"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "postinstall": "umi generate tmp",
        "dev": "umi dev",
        "start": "umi dev",
        "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 UMI_ENV=build umi build",
        "lint": "run-p lint:*",
        "lint:style": "prettier --check src/",
        "format": "run-p format:*",
        "format:style": "prettier --write src/",
        "test": "umi-test",
        "test:coverage": "umi-test --coverage",
        "prepublishOnly": "npm run build",
        "release": "bumpp --commit --push --tag && npm publish"
    },
    "dependencies": {
        "@ahooksjs/use-url-state": "^2.5.12",
        "@ant-design/icons": "^4.8.0",
        "@ant-design/pro-layout": "^6.38.22",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.19",
        "antd": "^4.24.10",
        "browserslist": "^4.21.5",
        "caniuse-lite": "^1.0.30001487",
        "dayjs": "^1.11.7",
        "dva-core": "^2.0.4",
        "highcharts": "^8.2.2",
        "highcharts-react-official": "^3.2.0",
        "lodash": "^4.17.21",
        "react": "~17.0.2",
        "react-dom": "~17.0.2",
        "react-highlight-words": "^0.17.0",
        "react-router": "6.3.0",
        "redbox-react": "1.6.0",
        "redux": "~4.2.1",
        "umi": "^3.5.40",
        "umi-request": "^1.4.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.194",
        "@types/react": "^17.0.59",
        "@types/react-dom": "^17.0.20",
        "@types/react-highlight-words": "^0.16.4",
        "@types/uuid": "^8.3.4",
        "@umijs/preset-react": "~1.8.32",
        "@umijs/test": "^3.5.40",
        "bumpp": "^9.1.0",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "pnpm": "^7.32.4",
        "prettier": "^2.8.8",
        "typescript": "^4.9.5"
    }
